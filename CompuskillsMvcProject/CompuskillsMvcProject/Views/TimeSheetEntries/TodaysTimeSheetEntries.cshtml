@model IEnumerable<MvcProjectDbConn.TimeSheetEntry>

@{
    ViewBag.Title = "TodaysTimeSheetEntries";
}

<h2>TodaysTimeSheetEntries</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Project.ProjectName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Client.ClientName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Project.ProjectName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Client.ClientName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndTime)
        </td>
        <td>
            @Html.ActionLink("Punch Out From this job", "PunchOut","TimeSheetEntries" ,new { id=item.TimeSheetEntryId }) |
            @Html.ActionLink("Details", "Details", new { id=item.TimeSheetEntryId }) |
        
        </td>
    </tr>
}

</table>
